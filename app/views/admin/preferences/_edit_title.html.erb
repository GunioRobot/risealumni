<% form_for(:title,:url => update_title_admin_preferences_path,:html =>{:method => :put}) do |f| %>
  <% @titles.each_with_index do|title, index| %>
    <% fields_for "titles[#{index}]",title do |f| %>
      <%= hidden_field_tag "titles[#{index}][id]" ,title.id %>
      <%= f.labelled_text_field "", :name, :title => "Title",:class => "text_field", :wrap_class => :row_complete %>
    <%end%>
  <%end%>
  <button class="buttons" type="submit">
    <%= image_tag('Update.png')%>
  </button>
<%end%>
<h1>New site_content</h1>
<% form_for @site_content, :url => admin_site_contents_path, :html => {:method=>:post, :class=>" "} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_site_contents_path %>

  <%= p.labelled_text_field('GTalk Name', :gtalk_name,:class => "gtalk_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Skype Name', :skype_name,:class => "skype_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Del.icio.us Name', :delicious_name,:class => "delicious_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>

<% rounded_med_corner(:id => "address_details", :title => "Web Info") do %>
  <%= p.labelled_text_field('Website', :website, :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Blog', :blog, :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Youtube Username',:youtube_username,:class => "youtube_icon input_text_with_icon",:wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Flickr Screen Name', :flickr_username,:class => "flickr_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('LinkedIn Name', :linkedin_name,:class => "linkedin_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
